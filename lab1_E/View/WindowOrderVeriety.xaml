<Window x:Class="lab1_E.View.WindowOrderVeriety"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:lab1_E.View"
      mc:Ignorable="d" 
      Title="Вид сделки" Height="450" Width="800">

    <StackPanel>
        <ListView x:Name="ListOrderVeriety">
            <ListView.View>
                <GridView>
                    <GridView.Columns>
                        <GridViewColumn Header="Код вида сделки" DisplayMemberBinding="{Binding Id}"/>
                        <GridViewColumn Header="Вид сделки" DisplayMemberBinding="{Binding Veriety}"/>
                    </GridView.Columns>
                </GridView>
            </ListView.View>
        </ListView>
    </StackPanel>
</Window>
